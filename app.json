{
  "expo": {
    "name": "Dear Days",
    "displayName": "Dear Days",
    "slug": "dear-days",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/logo/dear_days.png",
    "scheme": "deardays",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.janadev.deardays"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFD5ED",
        "foregroundImage": "./assets/logo/dear_days.png",
        "backgroundImage": "./assets/logo/dear_days.png",
        "monochromeImage": "./assets/logo/dear_days.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.janadev.deardays"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo/dear_days.png",
          "backgroundColor": "#FFD5ED"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0b69ef09-f598-4fb0-8d68-0b22819fea5f"
      }
    }
  }
}
