{
  "expo": {
    "name": "dear-days",
    "slug": "dear-days",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/dear_days.png",
    "scheme": "deardays",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFD5ED",
        "foregroundImage": "./assets/logo/dear_days.png",
        "backgroundImage": "./assets/logo/dear_days.png",
        "monochromeImage": "./assets/logo/dear_days.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#b10667",
          "dark": {
            "backgroundColor": "#b10667"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
